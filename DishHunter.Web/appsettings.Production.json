{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Identity": {
        "SignIn": {
            "RequireConfirmedAccount": true,
            "RequireConfirmedEmail": true
        },
        "Password": {
            "RequiredLength": 8,
            "RequireDigit": true,
            "RequiredUniqueChars": 1,
            "RequireLowercase": true,
            "RequireNonAlphanumeric": true,
            "RequireUppercase": true
        }
    }
}